{
    "env": {
        "browser": true,
        "es6": true,
        "webextensions": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": 0,
        "no-unused-vars": ["warn", { "vars": "all", "args": "all" } ],
        "no-undef": ["warn"],
        "no-proto": ["error"],
        "prefer-arrow-callback": ["warn"],
        "prefer-spread": ["warn"]
    }
}